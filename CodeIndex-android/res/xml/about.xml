<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <Preference
        android:key="about_appname"
        android:summary="@string/app_name"
        android:title="@string/about_appname_title" />
    <Preference
        android:key="about_version"
        android:title="@string/about_version_title" />
    <Preference
        android:key="about_developer"
        android:summary="@string/about_developer_name"
        android:title="@string/about_developer_title" >
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/about_developer_url" />
    </Preference>
    <Preference
        android:key="about_appwebpage"
        android:summary="@string/about_appwebpage_url"
        android:title="@string/about_appwebpage_title" >
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/about_appwebpage_url" />
    </Preference>
    <Preference
        android:key="about_moreapp"
        android:title="@string/about_moreapp_title" >
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/about_moreapp_url" />
    </Preference>
    <Preference
        android:key="about_email"
        android:summary="@string/about_email_subtitle"
        android:title="@string/about_email_title" >
        <intent android:action="android.intent.action.SEND" />
    </Preference>
    <Preference
        android:key="about_share"
        android:title="@string/about_share_title" />
    <Preference
        android:key="about_jni"
        android:title="@string/about_jni_title" />
    <Preference
        android:key="about_share"
        android:summary="@string/i18n_version"
        android:title="@string/about_i18n_version_title" />

</PreferenceScreen>